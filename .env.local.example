# DeepTrust Environment Variables
# Copy this file to .env.local and fill in your API keys

# Deepfake Detection API
VITE_DEEPFAKE_API_KEY=your_api_key_here
VITE_DEEPFAKE_API_ENDPOINT=https://api.example.com/deepfake

# Finance Document API
VITE_FINANCE_API_KEY=your_api_key_here
VITE_FINANCE_API_ENDPOINT=https://api.example.com/finance

# OCR API
VITE_OCR_API_KEY=your_api_key_here
VITE_OCR_API_ENDPOINT=https://api.example.com/ocr

# Google Gemini API
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Demo Mode (set to true for mock data)
VITE_DEMO_MODE=true

